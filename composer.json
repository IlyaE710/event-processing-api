        "yiisoft/yii2": "~2.0.45",
        "yiithings/yii2-dotenv": "*",
        "predis/predis": "^2.2",
        "ext-pcntl": "*",
        "ext-redis": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
